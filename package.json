{
  "name": "vpnapi-client",
  "version": "0.1.3",
  "description": "A Node.js client for the VPNAPI.io.",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "example": "node-ts ./example/main.ts",
    "build": "rm -fr dist/* && npm run build:esm && npm run build:cjs && cp package.json README.md LICENSE dist/",
    "build:esm": "tsc -p tsconfig.json && echo '{\"type\":\"module\"}' > dist/esm/package.json",
    "build:cjs": "tsc -p tsconfig.cjs.json && echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json",
    "test": "vitest",
    "prettier": "prettier --write .",
    "lint": "eslint . *.ts",
    "lint:fix": "eslint . --fix *.ts"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "vpnapi",
    "vpnapi.io",
    "vpnapi-client",
    "vpnapi-node"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frauniki/vpnapi-client.git"
  },
  "bugs": {
    "url": "https://github.com/frauniki/vpnapi-client/issues"
  },
  "homepage": "https://github.com/frauniki/vpnapi-client",
  "author": "frauniki <frauniki@sinoa.jp>",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
    "@types/node": "^22.10.1",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.13.0",
    "prettier": "^3.4.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0",
    "vitest": "^2.1.7"
  },
  "dependencies": {
    "axios": "^1.7.8",
    "axios-case-converter": "^1.1.1",
    "axios-retry": "^4.5.0"
  }
}
